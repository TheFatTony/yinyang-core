<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="thefattony" id="mail_queue-id-index">
        <createIndex indexName="mail_queue_id_idx" tableName="mail_queue" unique="true">
            <column name="id" type="bigint"/>
        </createIndex>
    </changeSet>

    <changeSet author="thefattony" id="mail_queue-effective_date-index">
        <createIndex indexName="mail_queue_effective_date_idx" tableName="mail_queue" unique="true">
            <column name="effective_date" type="datetime"/>
        </createIndex>
    </changeSet>

</databaseChangeLog>

