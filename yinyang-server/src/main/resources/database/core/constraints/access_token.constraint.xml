<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9 http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="thefattony" id="access_token-id-nn">
        <addNotNullConstraint columnDataType="bigint" columnName="id" tableName="access_token"/>
    </changeSet>

    <changeSet author="thefattony" id="access_token-token-nn">
        <addNotNullConstraint columnDataType="varchar(255)" columnName="token" tableName="access_token"/>
    </changeSet>

    <changeSet author="thefattony" id="access_token-id-aa">
        <addAutoIncrement columnDataType="bigint" columnName="id" tableName="access_token"/>
    </changeSet>

</databaseChangeLog>

